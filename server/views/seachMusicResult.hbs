{{#if error}}
<h3>{{error}}</h3>
{{else}} {{#if netease}}
<ul>
  {{#each seachResult.result.songs}}
  <li>
    <p>
      <span>歌曲名称 :
        <a href="/music/radio?id={{this.id}}">{{this.name}}</a>
      </span>
      <img class="musicPic" src="{{this.al.picUrl}}" alt="歌曲图片" width="100" height="80">
    </p>
    <p>歌曲id : {{this.id}}
    </p>
    {{#if this.alia}}
    <p>歌曲别名 : {{this.alia}}</p>
    {{/if}}
    <p>专辑 :
      <span>{{this.al.name}}</span>
    </p>
    <p>
      <span>热度 : {{this.pop}}</span>
    </p>
    <p>歌手 : </p>
    <ul>
      {{#each this.ar}}
      <li> {{this.name}}</li>
      {{!--
      <li>歌手id : {{this.id}}</li> --}}
      <br> {{/each}}
    </ul>
  </li>
  <hr class="hr"> {{/each}}
</ul>
{{!-- <p>
  {{resultStr}}
</p> --}}
{{/if}} {{#if qq}} {{#each seachResult.data.song.list}}
<li>
  <p>
    <span>音乐 : </span>
    <span>
      <a href="http://ws.stream.qqmusic.qq.com/C400{{this.songmid}}.m4a?fromtag=0&guid=2458413055">{{this.songname}}</a>
    </span>
  </p>
  <img src="https://y.gtimg.cn/music/photo_new/T002R300x300M000{{this.albummid}}.jpg?max_age=2592000" alt="" width="100" height="80">
  <p>
    <span>专辑 : </span>
    <span>{{this.albumname}}</span>
  </p>
  <p>歌手 : </p>
  <ul> {{#each this.singer}}
    <li
      <p>{{this.name}}</p>
    </li>
    {{/each}}
  </ul>
</li>
{{#if this.songurl}}
<p>
  <span>QQ音乐地址 :
    <a href="{{this.songurl}}">{{this.songurl}}</a>
  </span>
</p>
{{/if}}
<hr> {{/each}} {{/if}} {{!--
<p>{{resultStr}}</p> --}} {{/if}}